{
    "name": "@ljharb/resumer",
    "version": "0.0.0",
    "description": "a through stream that starts paused and resumes on the next tick",
    "main": "index.js",
    "dependencies": {
        "through": "~2.3.4"
    },
    "devDependencies": {
        "concat-stream": "^0.1.1",
        "tape": "^5.6.5"
    },
    "scripts": {
        "test": "tape 'test/**/*.js'"
    },
    "testling": {
        "files": "test/*.js",
        "browsers": [
            "ie/6..latest",
            "chrome/20..latest",
            "firefox/10..latest",
            "safari/latest",
            "opera/11.0..latest",
            "iphone/6",
            "ipad/6"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/ljharb/resumer.git"
    },
    "homepage": "https://github.com/ljharb/resumer",
    "keywords": [
        "through",
        "stream",
        "pause",
        "resume"
    ],
    "author": {
        "name": "James Halliday",
        "email": "mail@substack.net",
        "url": "http://substack.net"
    },
    "license": "MIT"
}
